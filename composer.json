{
    "name": "sksergey/dataforseo_client_v2",
    "description": "A PHP wrapper for using DataForSEO API; fork of jovixv/dataforseo_client",
    "keywords": [
        "dataforseo", "api", "seo", "dataforseo client", "seo tools", "client for dataforseo", "php wrapper dataforseo", "dataforseo api", "DFS API", "api seo", "seo api php", "google search volume php"
    ],
    "homepage": "https://github.com/jovixv/dataforseo_client",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitaliy Lyabakh",
            "email": "igrolan@gmail.com",
            "homepage": "https://dataforseo.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "alpha",
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": ">=7.0",
        "symfony/var-dumper": ">=5.0"
    },

    "autoload": {
        "psr-0":{
            "DFSClient\\": "DFSClient/"
        },
        "psr-4": {
            "DFSClient\\": "DFSClient/"
        }
    },

    "extra": {
        "laravel":{
            "providers":[
                "DFSClient\\Provider\\DfsServiceProvider"
            ]
        }
    }

}
